    "prestart": "npm install",
    "pretest": "npm install",
    "start local": "http-server -a localhost.com -p 5001",
    "test-single-run": "karma start karma.conf.js --single-run",
    "test-karma": "./node_modules/karma/bin/karma start karma.conf.js --single-run --browsers PhantomJS",
